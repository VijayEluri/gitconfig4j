Utility library for git-like configuration files support on Java

Example
--------
[core]  
        autocrlf = false
	excludesfile = /home/timoteo/.gitignore
	editor = emacs
	
[alias]
        last = cat-file commit HEAD
		
[user]
        name = Timoteo Ponce
	email = timo.slack@gmail.com

[color]
        diff = auto
	status = auto
	branch = auto
	interactive = auto

[remote "origin"]
        url = http://git.kernel.org/pub/scm/git/git.git
	fetch = +refs/heads/*:refs/remotes/origin/*
